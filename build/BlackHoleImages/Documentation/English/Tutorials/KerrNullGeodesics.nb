Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["\[Theta]", "e"], ",", 
     SubscriptBox["\[Phi]", "e"]}], "}"}], " ", "=", " ", 
   RowBox[{"geod", "[", "\"\<EscapeCoordinates\>\"", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Theta]o", " ", "=", " ", 
   RowBox[{"\[Pi]", "/", "3"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Delta]", " ", "=", " ", 
   RowBox[{"ArcCos", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       RowBox[{"Sin", "[", 
        SubscriptBox["\[Theta]", "e"], "]"}]}], 
      RowBox[{"Sin", "[", 
       RowBox[{"\[Pi]", "-", "\[Theta]o"}], "]"}], " ", 
      RowBox[{"Cos", "[", 
       SubscriptBox["\[Phi]", "e"], "]"}]}], " ", "+", " ", 
     RowBox[{
      RowBox[{"Cos", "[", 
       SubscriptBox["\[Theta]", "e"], "]"}], " ", 
      RowBox[{"Cos", "[", 
       RowBox[{"\[Pi]", "-", "\[Theta]o"}], "]"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{
  "\"\<\[Delta] = \>\"", ",", " ", "\[Delta]", ",", " ", "\"\< rad\>\""}], 
  "]"}]}], "Input",
