Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Spherical2Cartesian", "[", "list_", "]"}], ":=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"list", "[", 
       RowBox[{"[", "2", "]"}], "]"}], " ", 
      RowBox[{"Sin", "[", 
       RowBox[{"list", "[", 
        RowBox[{"[", "3", "]"}], "]"}], "]"}], " ", 
      RowBox[{"Cos", "[", 
       RowBox[{"list", "[", 
        RowBox[{"[", "4", "]"}], "]"}], "]"}]}], ",", " ", 
     RowBox[{
      RowBox[{"list", "[", 
       RowBox[{"[", "2", "]"}], "]"}], " ", 
      RowBox[{"Sin", "[", 
       RowBox[{"list", "[", 
        RowBox[{"[", "3", "]"}], "]"}], "]"}], " ", 
      RowBox[{"Sin", "[", 
       RowBox[{"list", "[", 
        RowBox[{"[", "4", "]"}], "]"}], "]"}]}], ",", " ", 
     RowBox[{
      RowBox[{"list", "[", 
       RowBox[{"[", "2", "]"}], "]"}], " ", 
      RowBox[{"Cos", "[", 
       RowBox[{"list", "[", 
        RowBox[{"[", "3", "]"}], "]"}], "]"}]}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"plotA", " ", "=", " ", 
   RowBox[{"ParametricPlot3D", "[", 
    RowBox[{
     RowBox[{"Spherical2Cartesian", "[", 
      RowBox[{"geod", "[", "\[Lambda]", "]"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Lambda]", ",", "0", ",", 
       SubscriptBox["\[Lambda]", "\[Infinity]"]}], "}"}], ",", 
     RowBox[{"PlotPoints", "->", "1000"}], ",", " ", 
     RowBox[{"PlotRange", "->", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "15"}], ",", "15"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "15"}], ",", "15"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "15"}], ",", "15"}], "}"}]}], "}"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"plotB", "=", " ", 
   RowBox[{"SphericalPlot3D", "[", 
    RowBox[{
     RowBox[{"1", "+", 
      SqrtBox[
       RowBox[{"1", "-", 
